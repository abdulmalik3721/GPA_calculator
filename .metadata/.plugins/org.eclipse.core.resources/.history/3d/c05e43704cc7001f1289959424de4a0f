package GPA.java;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		//total points / total credits
		//points for a class=grade value*credits
		//A=4, B=3...
		
		Scanner scanner = new Scanner(System.in);
		
		
		System.out.println("Enter number of credits:");
		String credits=scanner.nextLine();
		
		
		System.out.println("Enter number of Grades:");
		String grades=scanner.nextLine();
		
		
		Integer points=0;
		Integer gradeValue=0;
		if(grades.equalsIgnoreCase("A")) {
			gradeValue=4;
		}
		else if (grades.endsWith("B")){
			gradeValue=3;
		}
		else if (grades.endsWith("C")){
			gradeValue=2;
		}
		else if (grades.endsWith("D")){
			gradeValue=1;
		}
		else if (grades.endsWith("F")){
			gradeValue=0;
		}
		
		
		
		
		
		
		
		
		System.out.println(credits);
		System.out.println(grades);
		
		
		scanner.close();
		
		
		

	}

}
